import requests

url = "在这里填写请求的URL"
headers = {
    "Content-Type": "application/json",
}
data = {
    "auth_token": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx",
    "from": "XXXXXXXXXXXXXXXXXXXXXXXX",
    "type": "text",
    "text": "文言送信\n問題ないはず",
}

response = requests.post(url, headers=headers, json=data)

print(response.text)
